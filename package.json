{
  "name": "mymarkdown",
  "description": "Markdown Online Editor",
  "version": "1.0.0",
  "author": "takumon <inouetakumon@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "analyze": "nuxt build --analyze",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test:unit": "TEST=unit ava --verbose",
    "storybook:start": "start-storybook -p 9001 -s ./static -c .storybook",
    "storybook:build": "build-storybook -s ./static -c .storybook -o docs"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^2.0.8",
    "@vue/test-utils": "^1.0.0-beta.16",
    "axios": "^0.18.0",
    "firebase": "^5.0.4",
    "highlightjs": "^9.10.0",
    "marked": "^0.4.0",
    "moment": "^2.22.2",
    "nuxt": "^1.0.0",
    "vue-material": "^1.0.0-beta-10.2",
    "vue-scroll": "^2.1.7"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.5",
    "@storybook/addon-centered": "^3.4.5",
    "@storybook/addon-knobs": "^3.4.5",
    "@storybook/addon-links": "^3.4.5",
    "@storybook/addon-notes": "^3.4.5",
    "@storybook/vue": "^3.4.6",
    "ajv": "^6.0.0",
    "ava": "^0.25.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "browser-env": "^3.2.5",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "prettier": "^1.13.4",
    "require-extension-hooks": "^0.3.2",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^1.0.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.21.0",
    "vue-loader": "^14.2.1",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/setup.js"
    ],
    "files": [
      "./test/specs/*.js"
    ],
    "snapshotDir": "./test/snapshots"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
